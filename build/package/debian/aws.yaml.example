# example configuration
region: us-west-2
api_endpoint: http://127.0.0.1:8080/api
sync_interval_in_seconds: 5
upstreams:
  - name: backend1
    autoscaling_group: backend-group-1
    port: 80
    kind: http
  - name: backend2
    autoscaling_group: backend-group-2
    port: 80
    kind: http
  - name: tcp-backend
    autoscaling_group: backend-group-3
    port: 80
    kind: stream
